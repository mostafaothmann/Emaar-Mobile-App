{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "nativewind/types"
    ]
  },
  "include": [
    "App.tsx",
    "src",
    "types.d.ts",
    "declarations.d.ts",
    "nativewind-env.d.ts"
  ]
}